        successRate: 0\n      }\n    };\n  },\n  computed: {\n    // garde toutes les computed existantes (filteredMissions, sortedMissions, paginatedMissions...)\n  },\n  methods: {\n    // --- M√©thode pour charger les missions depuis l'API ---\n    loadMissions: function loadMissions() {\n      var _this = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee() {\n        var response, _t;\n        return _regenerator().w(function (_context) {\n          while (1) switch (_context.p = _context.n) {\n            case 0:\n              _context.p = 0;\n              _context.n = 1;\n              return getMissions();\n            case 1:\n              response = _context.v;\n              _this.missions = response.data.missions.map(function (m) {\n                return _objectSpread(_objectSpread({}, m), {}, {\n                  hasApplied: false // ajoute ce champ pour le suivi local\n                });\n              });\n              _context.n = 3;\n              break;\n            case 2:\n              _context.p = 2;\n              _t = _context.v;\n              console.error('Erreur lors du chargement des missions:', _t);\n            case 3:\n              return _context.a(2);\n          }\n        }, _callee, null, [[0, 2]]);\n      }))();\n    } // conserve toutes les m√©thodes existantes (toggleQuickFilter, applyToMission, submitApplication...)\n  },\n  mounted: function mounted() {\n    this.loadMissions(); // üîπ Charger les missions au montage du composant\n  },\n  watch: {\n    // garde les watchers existants\n  }\n};","map":{"version":3,"names":["getMissions","name","props","data","searchQuery","activeQuickFilter","sortBy","currentPage","itemsPerPage","filters","minBudget","maxBudget","skills","types","skillFilter","showApplicationModal","selectedMission","application","proposal","proposedBudget","deliveryTime","quickFilters","id","label","allSkills","missions","userStats","applicationsSent","interviews","suc