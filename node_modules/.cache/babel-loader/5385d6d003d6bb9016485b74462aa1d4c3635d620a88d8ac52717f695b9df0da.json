         _context2.n = 9;\n              break;\n            case 8:\n              _context2.p = 8;\n              _t = _context2.v;\n              console.error(\"Erreur lors du chargement du dashboard :\", _t);\n            case 9:\n              return _context2.a(2);\n          }\n        }, _callee2, null, [[0, 8]]);\n      }))();\n    },\n    updateStats: function updateStats() {\n      if (this.currentUser.role === 'FREELANCE') {\n        this.stats.activeMissions = this.recentMissions.filter(function (m) {\n          return m.status === 'IN_PROGRESS';\n        }).length;\n        this.stats.proposalsSent = this.recentMissions.length * 2; // EXEMPLE\n        this.stats.averageRating = 4.7; // EXEMPLE\n        this.stats.estimatedEarnings = 1500; // EXEMPLE\n      }\n      if (this.currentUser.role === 'CLIENT') {\n        this.stats.publishedMissions = this.clientMissions.length;\n        this.stats.activeMissions = this.clientMissions.filter(function (m) {\n          return m.status === 'IN_PROGRESS';\n        }).length;\n        this.stats.proposalsReceived = this.clientMissions.reduce(function (sum, m) {\n          return sum + (m.proposalsCount || 0);\n        }, 0);\n        this.stats.successRate = 92; // EXEMPLE\n      }\n    },\n    viewMission: function viewMission(missionId) {\n      this.$router.push(\"/missions/\".concat(missionId));\n    },\n    applyToMission: function applyToMission(missionId) {\n      alert(\"Candidature envoy\\xE9e pour la mission #\".concat(missionId));\n    },\n    manageProposals: function manageProposals(missionId) {\n      this.$router.push(\"/missions/\".concat(missionId, \"/proposals\"));\n    }\n  }\n};","map":{"version":3,"names":["getMission","getLatestOffers","getClientMissions","getRecommendedFreelancers","getFreelanceMissions","name","props","data","stats","activeMissions","proposalsSent","averageRating","estimatedEarnings","publishedMissions","proposalsReceived","successRate","latestOffers","recentMissions","cl