veNewPortfolioItem","_hoisted_7","deletePortfolioItem","_hoisted_8","addPortfolioItem","resetForm","disabled","isFormValid","_hoisted_9","join"],"sources":["/home/christ/T√©l√©chargements/projet 1.2/src/views/FreelanceProfileView.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Header et progression -->\n    <div>\n      <h1>Mon Profil Freelance</h1>\n      <p>Compl√©tez votre profil pour attirer plus de clients et d√©crocher des missions.</p>\n      <div v-if=\"completionPercentage < 100\">\n        Profil compl√©t√© √† {{ completionPercentage }}%\n      </div>\n    </div>\n\n    <!-- Formulaire du profil -->\n    <form @submit.prevent=\"saveProfile\">\n      <!-- Informations personnelles -->\n      <h2>Informations personnelles</h2>\n      <div>\n        <label>Pr√©nom *</label>\n        <input v-model=\"profile.first_name\" required />\n      </div>\n      <div>\n        <label>Nom *</label>\n        <input v-model=\"profile.last_name\" required />\n      </div>\n      <div>\n        <label>Titre professionnel *</label>\n        <input v-model=\"profile.title\" required />\n      </div>\n      <div>\n        <label>Description *</label>\n        <textarea v-model=\"profile.description\" rows=\"4\" required></textarea>\n      </div>\n\n      <!-- Comp√©tences -->\n      <h2>Comp√©tences</h2>\n      <div>\n        <input\n          v-model=\"newSkill\"\n          @keydown.enter.prevent=\"addSkill\"\n          placeholder=\"Nouvelle comp√©tence\"\n        />\n        <button type=\"button\" @click=\"addSkill\">Ajouter</button>\n      </div>\n      <div v-for=\"(skill, index) in profile.skills\" :key=\"index\">\n        {{ skill }}\n        <button type=\"button\" @click=\"removeSkill(index)\">Supprimer</button>\n      </div>\n\n      <!-- Tarifs et disponibilit√© -->\n      <h2>Tarifs et disponibilit√©</h2>\n      <div>\n        <label>Taux horaire (‚Ç¨) *</label>\n        <input type=\"number\" v-model.number=\"profile.hourly_rate\" required />\n      </div>\n      <div>\n        <label>Disponibilit√© *</label>\n        <select v-model=\"profile.availability\" required>\n          <option value=\"AVAILABLE\">Disponible</option>\n          <option value=\"PART_TIME\">Temps partiel</option>\n          <option value=\"BUSY\">Occup√©</option>\n          <option value=\"UNAVAILABLE\">Indisponible</option>\n        </select>\n      </div>\n\n      <!-- Portfolio -->\n      <h2>Portfolio</h2>\n      <div v-for=\"(item, index) in portfolio\" :key=\"item.id || index\">\n        <h3>Projet {{ index + 1 }}</h3>\n        <div>\n          <label>Titre</label>\n          <input v-model=\"item.title\" />\n        </div>\n        <div>\n          <label>URL</label>\n          <input v-model=\"item.project_url\" type=\"url\" />\n        </div>\n        <div>\n          <label>Description</label>\n          <textarea v-model=\"item.description\" rows=\"3\"></textarea>\n        </div>\n        <div>\n          <button v-if=\"item.id\" type=\"button\" @click=\"updatePortfolioItem(item)\">\n            Mettre √† jour\n          </button>\n          <button v-else type=\"button\" @click=\"saveNewPortfolioItem(item)\">\n            Enregistrer\n          </button>\n          <button type=\"button\" @click=\"deletePortfolioItem(item, index)\">Supprimer</button>\n        </div>\n      </div>\n      <button type=\"button\" @click=\"addPortfolioItem\">Ajouter un projet</button>\n\n      <!-- Actions -->\n      <div>\n        <button type=\"button\" @click=\"resetForm\">Annuler</button>\n        <button type=\"submit\" :disabled=\"!isFormValid\">Sauvegarder le profil</button>\n      </div>\n    </form>\n\n    <!-- Aper√ßu simple du profil -->\n    <div>\n      <h2>Aper√ßu du profil</h2>\n      <p>{{ profile.first_name }} {{ profile.last_name }}</p>\n      <p>{{ profile.title }}</p>\n      <p>{{ profile.description }}</p>\n      <p>Comp√©tences: {{ profile.skills.join(', ') }}</p>\n      <p>Taux horaire: {{ profile.hourly_rate }}‚Ç¨</p>\n      <p>Disponibilit√©: {{ profile.availability }}</p>\n      <h3>Portfolio</h3>\n      <ul>\n        <li v-for=\"item in portfolio\" :key=\"item.id || item.title\">\n          {{ item.title }} - {{ item.project_url }}\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  updateProfile,\n  getFreelanceProfile,\n  getPortfolioItems,\n  addPortfolioItem,\n  updatePortfolioItem,\n  deletePortfolioItem,\n} from '@/services/api';\n\nexport default {\n  name: 'FreelanceProfileView',\n  props: ['currentUser'],\n  data() {\n    return {\n      profile: {\n        first_name: '',\n        last_name: '',\n        title: '',\n        description: '',\n        skills: [],\n        hourly_rate: null,\n        availability: 'AVAILABLE',\n      },\n      portfolio: [],\n      newSkill: '',\n      loading: false,\n      existingProfileId: null,\n    };\n  },\n  computed: {\n    isPersonalInfoComplete() {\n      return (\n        this.profile.first_name &&\n        this.profile.last_name &&\n        this.profile.title &&\n        this.profile.description\n      );\n    },\n    completionPercentage() {\n      let completed = 0;\n      const totalFields = 6;\n      if (this.profile.first_name) completed++;\n      if (this.profile.last_name) completed++;\n      if (this.profile.title) completed++;\n      if (this.profile.description) completed++;\n      if (this.profile.skills.length >= 3) completed++;\n      if (this.profile.hourly_rate) completed++;\n      return Math.round((completed / totalFields) * 100);\n    },\n    isFormValid() {\n      return (\n        this.profile.first_name &&\n        this.profile.last_name &&\n        this.profile.title &&\n        this.profile.description &&\n        this.profile.skills.length >= 3 &&\n        this.profile.hourly_rate !== null &&\n        this.profile.availability\n      );\n    },\n  },\n  methods: {\n    addSkill() {\n      const skill = this.newSkill.trim();\n      if (skill && !this.profile.skills.includes(skill)) {\n        this.profile.skills.push(skill);\n        this.newSkill = '';\n      }\n    },\n    removeSkill(index) {\n      this.profile.skills.splice(index, 1);\n    },\n    addPortfolioItem() {\n      this.portfolio.push({ title: '', description: '', project_url: '', id: null });\n    },\n    async saveNewPortfolioItem(item) {\n      if (!item.title.trim()) return alert('Le titre du projet est obligatoire');\n      try {\n        const response = await addPortfolioItem(this.existingProfileId, item);\n        item.id = response.data.id;\n        alert('‚úÖ Projet ajout√© !');\n      } catch (err) {\n        console.error(err);\n        alert('‚ùå Erreur lors de la cr√©ation du projet');\n      }\n    },\n    async updatePortfolioItem(item) {\n      try {\n        await updatePortfolioItem(this.existingProfileId, item.id, item);\n        alert('‚úÖ Projet mis √† jour !');\n      } catch (err) {\n        console.error(err);\n        alert('‚ùå Erreur lors de la mise √† jour');\n      }\n    },\n    async deletePortfolioItem(item, index) {\n      if (!item.id) return this.portfolio.splice(index, 1);\n      if (!confirm('Supprimer ce projet ?')) return;\n      try {\n        await deletePortfolioItem(this.existingProfileId, item.id);\n        this.portfolio.splice(index, 1);\n        alert('üóëÔ∏è Projet supprim√©');\n      } catch (err) {\n        console.error(err);\n        alert('‚ùå Erreur lors de la suppression');\n      }\n    },\n    async saveProfile() {\n      if (!this.isFormValid) return alert('Veuillez compl√©ter tous les champs obligatoires');\n      this.loading = true;\n      try {\n        const response = await updateProfile(this.currentUser.id, this.profile);\n        if (response.data) this.existingProfileId = response.data.id;\n        alert('‚úÖ Profil sauvegard√© !');\n      } catch (err) {\n        console.error(err);\n        alert('‚ùå Erreur de sauvegarde');\n      } finally {\n        this.loading = false;\n      }\n    },\n    async loadProfileData() {\n      try {\n        if (this.currentUser?.freelance_profile) {\n          const profile = this.currentUser.freelance_profile;\n          this.existingProfileId = profile.id;\n          this.profile = {\n            first_name: profile.first_name || '',\n            last_name: profile.last_name || '',\n            title: profile.title || '',\n            description: profile.description || '',\n            skills: profile.skills || [],\n            hourly_rate: profile.hourly_rate || null,\n            availability: profile.availability || 'AVAILABLE',\n          };\n          const { data } = await getPortfolioItems(profile.id);\n          this.portfolio = data || [];\n        }\n      } catch (err) {\n        console.error('Erreur chargement profil:', err);\n      }\n    },\n    resetForm() {\n      if (confirm('Annuler les modifications ?')) this.loadProfileData();\n    },\n  },\n  mounted() {\n    this.loadProfileData();\n  },\n};\n</script>\n\n\n<!-- \n<script>\nimport { createProfile, updateProfile, addPortfolioItem, getProfile } from '@/services/api';\n\nexport default {\n  name: 'FreelanceProfileView',\n  props: ['currentUser'],\n  data() {\n    return {\n      profile: {\n        first_name: '',\n        last_name: '',\n        title: '',\n        description: '',\n        skills: [],\n        hourly_rate: null,\n        availability: 'AVAILABLE'\n      },\n      portfolio: [],\n      newSkill: '',\n      loading: false,\n      existingProfileId: null\n    };\n  },\n  computed: {\n    isPersonalInfoComplete() {\n      return this.profile.first_name && this.profile.last_name && this.profile.title && this.profile.description;\n    },\n    completionPercentage() {\n      let completed = 0;\n      const totalFields = 6;\n      \n      if (this.profile.first_name) completed++;\n      if (this.profile.last_name) completed++;\n      if (this.profile.title) completed++;\n      if (this.profile.description) completed++;\n      if (this.profile.skills.length >= 3) completed++;\n      if (this.profile.hourly_rate) completed++;\n      \n      return Math.round((completed / totalFields) * 100);\n    },\n    isFormValid() {\n      return this.profile.first_name && \n             this.profile.last_name && \n             this.profile.title && \n             this.profile.description && \n             this.profile.skills.length >= 3 && \n             this.profile.hourly_rate && \n             this.profile.availability;\n    }\n  },\n  methods: {\n    addSkill() {\n      const skill = this.newSkill.trim();\n      if (skill && !this.profile.skills.includes(skill)) {\n        this.profile.skills.push(skill);\n        this.newSkill = '';\n      }\n    },\n    removeSkill(index) {\n      this.profile.skills.splice(index, 1);\n    },\n    addPortfolioItem() {\n      this.portfolio.push({\n        title: '',\n        description: '',\n        project_url: ''\n      });\n    },\n    removePortfolioItem(index) {\n      this.portfolio.splice(index, 1);\n    },\n    async saveProfile() {\n      if (!this.isFormValid) {\n        alert('Veuillez compl√©ter tous les champs obligatoires (au moins 3 comp√©tences)');\n        return;\n      }\n\n      this.loading = true;\n\n      try {\n        const profileData = {\n          ...this.profile,\n          skills: this.profile.skills\n        };\n        // TOUJOURS utiliser l'ID du profil pour la mise √† jour, pas l'ID de l'user\n\n          const response = await updateProfile(this.currentUser.id, profileData);\n          if (response.data) {\n            this.existingProfileId = response.data.id;\n          }\n        \n\n        // Sauvegarde des √©l√©ments du portfolio\n        if (this.existingProfileId) {\n          for (const item of this.portfolio) {\n            if (item.title.trim()) {\n              await addPortfolioItem(this.existingProfileId, item);\n            }\n          }\n        }\n\n        alert('‚úÖ Profil sauvegard√© avec succ√®s !');\n        \n      } cat