missions = _this5.missions.filter(function (m) {\n                return m.id !== missionId;\n              });\n              _this5.calculateStats();\n              _this5.$toast.success('Mission supprim√©e avec succ√®s');\n              _context4.n = 4;\n              break;\n            case 3:\n              _context4.p = 3;\n              _t2 = _context4.v;\n              console.error('Erreur suppression mission:', _t2);\n              _this5.$toast.error('Erreur lors de la suppression de la mission');\n            case 4:\n              return _context4.a(2);\n          }\n        }, _callee4, null, [[1, 3]]);\n      }))();\n    },\n    viewApplications: function viewApplications(missionId) {\n      this.$router.push(\"/missions/\".concat(missionId, \"/applications\"));\n    },\n    publishMission: function publishMission(missionId) {\n      var _this6 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee5() {\n        var _t3;\n        return _regenerator().w(function (_context5) {\n          while (1) switch (_context5.p = _context5.n) {\n            case 0:\n              _context5.p = 0;\n              _context5.n = 1;\n              return apiService.updateMissionStatus(missionId, 'PUBLISHED');\n            case 1:\n              _context5.n = 2;\n              return _this6.loadClientMissions();\n            case 2:\n              // Recharger les donn√©es\n              _this6.$toast.success('Mission publi√©e avec succ√®s !');\n              _context5.n = 4;\n              break;\n            case 3:\n              _context5.p = 3;\n              _t3 = _context5.v;\n              console.error('Erreur publication mission:', _t3);\n              _this6.$toast.error('Erreur lors de la publication de la mission');\n            case 4:\n              return _context5.a(2);\n          }\n        }, _callee5, null, [[0, 3]]);\n      }))();\n    },\n    toggleStatusFilter: function toggleStatusFilter(filterId) {\n      this.activeStatusFilter = this.activeStatusFilter === filterId ? null : filterId;\n    },\n    getStatusLabel: function getStatusLabel(status) {\n      var labels = {\n        DRAFT: 'Brouillon',\n        PUBLISHED: 'Publi√©e',\n        IN_PROGRESS: 'En cours',\n        COMPLETED: 'Termin√©e',\n        CANCELLED: 'Annul√©e'\n      };\n      return labels[status] || status;\n    },\n    getTypeLabel: function getTypeLabel(type) {\n      var labels = {\n        FULL_TIME: 'Temps plein',\n        PART_TIME: 'Temps partiel',\n        FREELANCE: 'Freelance'\n      };\n      return labels[type] || type;\n    },\n    truncateDescription: function truncateDescription(description) {\n      if (!description) return '';\n      return description.length > 150 ? description.substring(0, 150) + '...' : description;\n    },\n    formatDate: function formatDate(dateString) {\n      if (!dateString) return 'Non d√©finie';\n      return new Date(dateString).toLocaleDateString('fr-FR');\n    },\n    getDaysLeft: function getDaysLeft(deadline) {\n      if (!deadline) return '?';\n      var today = new Date();\n      var deadlineDate = new Date(deadline);\n      var diffTime = deadlineDate - today;\n      var diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n      return diffDays > 0 ? diffDays : 0;\n    }\n  }\n};\nimport { useCssVars as _useCssVars } from 'vue';\nvar __injectCSSVars__ = function __injectCSSVars__() {\n  _useCssVars(function (_ctx) {\n    return {\n      \"2f730ed1-completionPercentage\": _ctx.completionPercentage\n    };\n  });\n};\nvar __setup__ = __default__.setup;\n__default__.setup = __setup__ ? function (props, ctx) {\n  __injectCSSVars__();\n  return __setup__(props, ctx);\n} : __injectCSSVars__;\nexport default __default__;","map":{"version":3,"names":["apiService","CreateMissionForm","__default__","name","components","props","data","missions","loading","searchQuery","activeStatusFilter","sortBy","showCreateMissionModal","statusFilters","id","label","stats","t