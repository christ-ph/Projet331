template>\n\n  <div class=\"missions-container\">\n\n    <h2 class=\"title\">üìå Missions & Candidatures re√ßues</h2>\n\n    <!-- Aucune mission -->\n    <div v-if=\"missions.length === 0\" class=\"empty\">\n      Aucune mission n‚Äôa encore re√ßu de candidatures.\n    </div>\n\n    <!-- Liste des missions -->\n    <div v-else class=\"missions-list\">\n\n      <div \n        v-for=\"mission in missions\" \n        :key=\"mission.id\" \n        class=\"mission-card\"\n      >\n        <!-- Infos mission -->\n        <h3>{{ mission.title }}</h3>\n        <p>{{ mission.description }}</p>\n\n        <p><strong>Statut :</strong> {{ mission.status }}</p>\n        <p><strong>üìÖ Cr√©√©e le :</strong> {{ formatDate(mission.created_at) }}</p>\n\n        <h4>üì® Candidatures re√ßues ({{ mission.applications_count }})</h4>\n\n        <!-- Aucune candidature -->\n        <div v-if=\"!mission.applications || mission.applications.length === 0\" class=\"empty-app\">\n          Aucune candidature pour cette mission.\n        </div>\n\n        <!-- Liste des candidatures -->\n        <div v-else class=\"applications-list\">\n\n          <div \n            v-for=\"app in mission.applications\" \n            :key=\"app.id\" \n            class=\"application-card\"\n          >\n            <p><strong>üë§ Freelance :</strong> {{ app.freelance_name }}</p>\n            <p><strong>üí¨ Proposition :</strong> {{ app.proposal }}</p>\n            <p><strong>üí∞ Budget propos√© :</strong> {{ app.proposed_budget }} ‚Ç¨</p>\n            <p><strong>üìå Statut :</strong> {{ app.status }}</p>\n            <p><strong>üìÖ Date :</strong> {{ formatDate(app.created_at) }}</p>\n\n            <!-- Boutons d'action -->\n            <div class=\"application-actions\">\n              <button \n                @click=\"acceptApplication(mission.id, app.id)\" \n                class=\"btn btn-success btn-small\"\n              >\n                ‚úÖ Accepter\n              </button>\n\n              <button \n                @click=\"rejectApplication(mission.id, app.id)\" \n                class=\"btn btn-danger btn-small\"\n              >\n                ‚ùå Refuser\n              </button>\n\n              <button \n                @click=\"openDiscussion(app.freelance_id)\" \n                class=\"btn btn-primary btn-small\"\n              >\n                üí¨ Discussion\n              </button>\n            </div>\n          </div>\n\n        </div>\n      </div>\n\n    </div>\n\n  </div>\n\n</template>\n\n\n<script>\nimport { \n  getMissions, \n  applyToMission,\n  getMissionApplications,\n  getClientMissionsAppliedByFreelance,\n  getFreelanceActiveMissions,\n  updateApplicationStatus \n} from '@/services/api';\n\nexport default {\n  name: 'MissionsApplicationsView',\n  props: ['currentUser'],\n\n  data() {\n    return {\n      showDetails: false,\n      successMessage: \"\",\n      errorMessage: \"\",\n      latestOffers: [],\n      applications: [],\n      missions: [] // on utilise un seul tableau coh√©rent\n    };\n  },\n\n  methods: {\n    // -----------------------\n    // ‚úî R√©cup√®re les derni√®res missions publi√©es\n    // -----------------------\n    async fetchLatestOffers() {\n      try {\n        const response = await getMissions();\n        this.latestOffers = response.data;\n      } catch (error) {\n        console.error('Error fetching latest offers:', error);\n      }\n    },\n\n    // -----------------------\n    // ‚úî Postuler √† une mission\n    // -----------------------\n    async applyToMission(missionId) {\n      try {\n        await applyToMission(missionId);\n        alert('Application sent successfully!');\n      } catch (error) {\n        console.error('Error applying to mission:', error);\n        alert('Failed to apply to mission.');\n      }\n    },\n\n    // -----------------------\n    // ‚úî R√©cup√®re les missions du client + candidatures\n    // -----------------------\n    async fetchClientMissions() {\n      try {\n        const clientId = this.currentUser ? this.currentUser.id : localStorage.getItem(\"user_id\");\n\n        const response = await getClientMissionsAppliedByFreelance(clientId);\n\n        // R√©ponse correcte\n        this.missions = response.data.missions_with_applications || [];\n      } catch (error) {\n        console.error(\"Erreur chargement missions client :\", error);\n        this.errorMessage = \"Impossible de charger les missions.\";\n      }\n    },\n\n    // -----------------------\n    // ‚úî Voir les candidatures d'une mission\n    // -----------------------\n    async viewMission(missionId) {\n      this.showDetails = true;\n      try {\n        const response = await getMissionApplications(missionId);\n        this.applications = response.data;\n      } catch (error) {\n        console.error('Error fetching mission applications:', error);\n      }\n    },\n\n    formatDate(dateString) {\n      if (!dateString) return 'N/A';\n      return new Date(dateString).toLocaleDateString('fr-FR', {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n\n    // -----------------------\n    // ‚úî Accepter une candidature\n    // -----------------------\n    async acceptApplication(missionId, appId) {\n      try {\n        console.log(`[DEBUG] Tentative d'acceptation de la candidature ID: ${appId} pour la mission ID: ${missionId}`);\n        const response = await updateApplicationStatus(appId, { status: \"ACCEPTED\" });\n        this.successMessage = response.data.message;\n        alert(this.successMessage);\n\n        await this.fetchClientMissions();\n      } catch (error) {\n        console.error(\"Erreur acceptation :\", error);\n        this.errorMessage = \"Impossible d‚Äôaccepter la candidature.\";\n      }\n    },\n\n    // -----------------------\n    // ‚úî Refuser une candidature\n    // -----------------------\n    async rejectApplication(missionId, appId) {\n      try {\n        console.log(`[DEBUG] Tentative d'acceptation de la candidature ID: ${appId} pour la mission ID: ${missionId}`);\n        const response = await updateApplicationStatus(appId, \"REJECTED\");\n        this.successMessage = response.data.message;\n        alert(this.successMessage);\n\n        await this.fetchClientMissions();\n      } catch (error) {\n        console.error(\"Erreur refus :\", error);\n        this.errorMessage = \"Impossible de refuser la candidature.\";\n      }\n    },\n\n    openDiscussion(freelanceId) {\n      console.log(`Ouverture de la discussion avec le freelance ${freelanceId}`);\n    },\n  },\n\n  mounted() {\n    this.fetchClientMissions();\n    this.fetchLatestOffers();\n  }\n};\n</script>\n\n\n<!-- <script>\nimport { getMissions, applyToMission ,getMissionApplications,getClientMissionsAppliedByFreelance,getFreelanceActiveMissions,updateApplicationStatus} from '@/services/api'; // Assure-toi que le chemin est correct\n\nexport default {\n  name: 'MissionsApplicationsView',\n  props: ['currentUser'],\n  data() {\n    return {\n        showDetails: false,\n        messagessucess:\"\",\n      latestOffers: [],\n        applications: [],\n        missionslist: [],\n        missions:[]\n    };\n  },\n  methods: {\n    async fetchLatestOffers() {\n      try {\n        const response = await getMissions();\n        this.latestOffers = response.data;\n      } catch (error) {\n        console.error('Error fetching latest offers:', error);\n      }\n    },\n    async applyToMission(missionId) {\n      try {\n        await applyToMission(missionId);\n       alert('Application sent successfully!');\n      } catch (error) {\n        console.error('Error applying to mission:', error);\n        alert('Failed to apply to mission.');\n      }\n    },\n    async loadMissionclientpostulepasfreelance(){\n        try{\n            const mis = await getClientMissionsAppliedByFreelance(this.currentUser.id)\n            this.missionslist = mis.data.missions_with_applications || [];\n            for(let i=0;i<this.missionslist.length;i++){\n                this.missions[i]=this.missionslist[i];\n            }\n             \n          \n        \n        }catch(error){\n            alert(\"erreur lors du chagement des missions\")\n        }\n\n    },\n    async viewMission(missionId) {\n      this.showDetails = true;\n      try {\n        const response = await getMissionApplications(missionId);\n        this.applications = response.data;\n      } catch (error) {\n        console.error('Error fetching mission applications:', error);\n      }\n    },\n    formatDate(dateString) {\n      if (!dateString) {\n        return 'N/A';\n      }\n      // Utilisation d'un objet Date pour le formatage\n      const options = { year: 'numeric', month: 'long', day: 'numeric', hour: '2-digit', minute: '2-digit' };\n      return new Date(dateString).toLocaleDateString('fr-FR', options);\n    },\n\n  async acceptApplication(missionId, appId) {\n    try {\n      const response = await updateApplicationStatus(appId, \"ACCEPTED\");\n\n      console.log(\"R√©ponse API :\", response.data);\n      this.successMessage = response.data.message;\n\n      // On recharge les missions de ce client\n      await this.fetchClientMissions();\n\n    } catch (error) {\n      console.error(\"Erreur acceptation :\", error);\n      this.errorMessage = \"Impossible d‚Äôaccepter la candidature.\";\n    }\n  },\n\n  // ‚úî M√©thode : refuser une candidature\n  async rejectApplication(missionId, appId) {\n    try {\n      const response = await updateApplicationStatus(appId, \"REJECTED\");\n\n      console.log(\"R√©ponse API :\", response.data);\n      this.successMessage = response.data.message;\n\n      // Mise √† jour des missions\n      await this.fetchClientMissions();\n\n    } catch (error) {\n      console.error(\"Erreur refus :\", error);\n      this.errorMessage = \"Impossible de refuser la candidature.\";\n    }\n  },\n\n  // ‚úî R√©cup√©rer la liste √† jour des missions + candidatures\nasync fetchClientMissions() {\n  const clientId = localStorage.getItem(\"user_id\");\n\n  const response = await getClientMissionsAppliedByFreelance(clientId);\n\n  this.missions = response.data.missions_with_applications;\n},\n\n    openDiscussion(freelanceId) {\n      console.log(`Ouverture de la discussion avec le freelance ${freelanceId}`);\n      // Logique de navigation/redirection vers la messagerie...\n    },\n  },\n  mounted() {\n    this.loadMissionclientpostulepasfreelance();\n    this.fetchLatestOffers();\n    \n  },\n};\n</script> -->\n\n<style scoped>  \n.mission-applications-view {\n  padding: 20px;\n}       \n.content-section {\n  margin-bottom: 40px;\n}\n.section-header {\n  display: flex;\n  justify-content: space-between;       \n}\n.offers-list {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n.offer-card {\n  border: 1px solid #ddd;\n  border-radius: 8px;\n  padding: 15px;\n  width: calc(33% - 20px);\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n}\n.offer-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 10px;\n}\n.budget {\n  font-weight: bold;\n  color: #28a745;\n}\n.offer-description {\n  margin-bottom: 10px;\n}\n.offer-meta {\n  font-size: 0.9em;\n  color: #666;\n  margin-bottom: 15px;\n}\n.offer-actions {\n  display: flex;                \n  justify-content: space-between;\n}\n.btn-small {\n  padding: 5px 10px;\n  font-size: 0.9em;\n}\n</style>"],"mappings":";;;;EAEO,SAAM;AAAoB;;;EAKK,SAAM;;;EAK5B,SAAM;AAAe;;;EAiB0C,SAAM;;;EAKjE,SAAM;AAAmB;;EAc5B,SAAM;AAAqB;;;;;uBA9C1CA,mBAAA,CA2EM,OA3ENC,UA2EM,G,0BAzEJC,mBAAA,CAAwD;IAApD,SAAM;EAAO,GAAC,mCAAiC,qBAEnDC,mBAAA,oBAAuB,EACZC,KAAA,CAAAC,QAAQ,CAACC,MAAM,U,cAA1BN,mBAAA,CAEM,OAFNO,UAEM,EAF0C,mDAEhD,M,cAGAP,mBAAA,CA+DMQ,SAAA;IAAAC,GAAA;EAAA,IAhENN,mBAAA,wBAA2B,EAC3BD,mBAAA,CA+DM,OA/DNQ,UA+DM,I,kBA7DJV,mBAAA,CA2DMQ,SAAA,QAAAG,WAAA,CA1DcP,KAAA,CAAAC,QAAQ,YAAnBO,OAAO;yBADhB